#!/bin/bash

https://www.google.com/search?q=site:$1 inurl:login,register,upload,logout,redirect,redir,goto,admin
https://www.google.com/search?q=site:$1 inurl:&
https://www.google.com/search?q=site:$1 ext:php,asp,aspx,jsp,jspa,txt,swf
https://www.google.com/search?q=site:$1 -$1 -$1
https://www.google.com/search?q=site:$1 filetype:txt
https://www.google.com/search?q=intitle:"index of / Parent Directory" site:$1
https://www.google.com/search?q=intitle:"Index of /admin" site:$1
https://www.google.com/search?q=intitle:"Index of /password" site:$1
https://www.google.com/search?q=intitle:"Index of /" passwd site:$1
https://www.google.com/search?q=intitle:"Index of /" .htaccess site:$1
https://www.google.com/search?q=Intitle: "login" "admin" site:$1
https://www.google.com/search?q=inurl:login site:$1
https://www.google.com/search?q=filetype:txt site:$1
